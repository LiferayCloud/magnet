{
  "name": "magnet",
  "version": "1.0.2",
  "description": "A modern approach for async microservices in node.js",
  "engines": {
    "node": ">=7.6.0"
  },
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp build:watch",
    "coverage": "nyc --reporter=lcov --include='src/**/*.js'  --reporter=text npm test",
    "lint": "gulp lint",
    "format": "prettier --single-quote --no-bracket-spacing --trailing-comma es5 --write \"{src/**/*.js,test/**/*.js,*.js}\" && eslint --fix \"{src/**/*.js,test/**/*.js,*.js}\"",
    "prepublish": "gulp build",
    "preversion": "npm test",
    "test": "gulp test",
    "test:watch": "gulp test:watch",
    "test:coverage:travis": "codecov"
  },
  "contributors": [
    "Ivan Santos <ivan.santos@liferay.com>",
    "Eduardo Lundgren <eduardo.lundgren@liferay.com>"
  ],
  "license": "BSD-3-Clause",
  "keywords": [
    "api",
    "express",
    "magnet",
    "rest"
  ],
  "main": "build/index.js",
  "bin": {
    "magnet": "build/bin/magnet"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wedeploy/magnet.git"
  },
  "dependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.0.0",
    "body-parser": "^1.13.1",
    "chokidar": "^1.6.1",
    "cli-table": "^0.3.1",
    "compression": "^1.6.2",
    "cross-spawn": "^5.0.1",
    "express": "^4.15.2",
    "express-validator": "^3.1.2",
    "fs-extra": "^2.0.0",
    "glob": "^7.1.1",
    "helmet": "^3.3.0",
    "jsdom": "^10.0.0",
    "magnet-plugin-controller": "^0.0.4",
    "magnet-plugin-function": "^0.0.8",
    "metal": "^2.6.4",
    "metal-assertions": "^2.8.0",
    "minimist": "^1.2.0",
    "morgan": "^1.6.0",
    "multer": "^1.3.0",
    "npmlog": "^4.0.2",
    "pretty-error": "^2.1.0",
    "resolve": "^1.3.3"
  },
  "devDependencies": {
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "eslint": "^3.17.1",
    "eslint-config-liferay": "^1.0.1",
    "eslint-loader": "^1.6.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-codecov": "^2.0.2",
    "gulp-eslint": "^3.0.1",
    "gulp-mocha": "^3.0.1",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "prettier": "^1.2.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0"
  }
}
